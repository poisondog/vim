# This is Git's per-user configuration file.
[user]
# Please adapt and uncomment the following lines:
	name = Adam Huang
	email = poisondog@gmail.com
[color]
	branch = auto
	diff = auto
	status = always

[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green

[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold

[color "status"]
	added = yellow
	changed = green
	untracked = cyan
